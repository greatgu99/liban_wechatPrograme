page {
  background-color: #f0efe9;
}

.box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  /* border: 1px solid pink; */
  align-items: center;
  margin:0;
  margin-top: 20rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  background-color: white;
}
.txt{
  /* border:1px solid blue; */
  letter-spacing: 0.1em;
  font-size: 30rpx;
}
.pic{
  width: 40rpx;
  height: 70rpx;
  /* border:1px solid green; */
}
.infoPic{
  border:1rpx solid #50e3a4;
  height: 100rpx;
  width: 100rpx;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}